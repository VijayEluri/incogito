<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

  <bean id="emsService" class="no.java.incogito.ems.client.RestEmsServiceFactoryBean">
    <property name="baseurl" value="${ems.url}"/>
  </bean>

  <bean id="voldemortHome" class="no.java.incogito.util.TestPathFactoryBean">
    <property name="testClass" value="no.java.incogito.application.IncogitoApplication"/>
    <property name="path" value="src/test/resources/cluster-${voldemort.clusterId}/node-${voldemort.nodeId}/voldemort"/>
  </bean>

  <bean id="incogitoHome" class="no.java.incogito.util.TestPathFactoryBean">
    <property name="testClass" value="no.java.incogito.application.IncogitoApplication"/>
    <property name="path" value="src/test/resources/cluster-${voldemort.clusterId}/node-${voldemort.nodeId}"/>
  </bean>

</beans>
